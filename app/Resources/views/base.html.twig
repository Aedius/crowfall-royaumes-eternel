<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Royaumes Eternels{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('css/normalize.css') }}" media="screen"/>
        <link rel="stylesheet" type="text/css" href="{{ asset('css/global.css') }}" media="screen"/>
    {% endblock %}
    {# START favicon #}
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="/manifest.json">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="theme-color" content="#ffffff">
    {# END favicon #}
</head>
<body>
{# header #}
<header>
    <img id="logo" src="{{ asset('images/global/Logo.png') }}">
    <img id="banniere" src="{{ asset('images/global/banniereTiny.png') }}">
</header>
<nav>
    {% block menu %}
        <a class="button" href="{{ path('homepage') }}">Home</a>
        <a class="button" href="{{ path('homepage') }}">News</a>
        <a class="button" href="{{ path('homepage') }}">Crows</a>
        <a class="button" href="{{ path('homepage') }}">Guides</a>
        <a class="button" href="{{ path('homepage') }}">Tools</a>
        <a class="button" href="{{ path('homepage') }}">Connect</a>
    {% endblock %}
</nav>
<main>
    {% block body %}{% endblock %}
</main>
<aside>
    {% block aside %}{% endblock %}
</aside>
{% block javascripts %}{% endblock %}
</body>
</html>
